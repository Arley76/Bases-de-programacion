Proceso ejercicio9
//Calcular el nuevo salario de un obrero, si obtuvo un incremento del 25% sobre su salario anterior.
	Definir  Vdia, NS Como Entero;
	Definir w, Tsaldo Como Real;
	Escribir "Digite su salario";
	Leer Vdia;
	w<- (0.25);
	Tsaldo <- (Vdia*w);  
    NS<- (Vdia+Tsaldo);
	Escribir  " El nuevo salario de la persona es $", NS ;	
	
FinProceso
